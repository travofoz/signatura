// Dropdown Toggle with Smooth Animation & Arrow Rotation
document.querySelectorAll(".toggle-btn").forEach(button => {
    button.addEventListener("click", function (event) {
        event.stopPropagation(); // Prevent parent click event

        let submenu = this.nextElementSibling;
        let isOpen = submenu.style.maxHeight && submenu.style.maxHeight !== "0px";

        // Close other dropdowns at the same level
        let parentMenu = this.closest("ul"); // Get parent UL
        parentMenu.querySelectorAll(".submenu").forEach(sub => {
            if (sub !== submenu) sub.style.maxHeight = "0px";
        });

        parentMenu.querySelectorAll(".toggle-btn").forEach(btn => {
            if (btn !== this) btn.classList.remove("rotate");
        });

        // Toggle current dropdown
        submenu.style.maxHeight = isOpen ? "0px" : submenu.scrollHeight + "px";
        this.classList.toggle("rotate", !isOpen);
    });
});

// Sub-dropdown (Nested Dropdown) Fix
document.querySelectorAll(".sub-toggle-btn").forEach(button => {
    button.addEventListener("click", function (event) {
        event.stopPropagation(); // Prevent parent click event

        let subSubmenu = this.nextElementSibling;
        let isOpen = subSubmenu.style.maxHeight && subSubmenu.style.maxHeight !== "0px";

        // Close other sub-dropdowns at the same level
        let parentSubmenu = this.closest(".submenu");
        parentSubmenu.querySelectorAll(".sub-submenu").forEach(sub => {
            if (sub !== subSubmenu) sub.style.maxHeight = "0px";
        });

        parentSubmenu.querySelectorAll(".sub-toggle-btn").forEach(btn => {
            if (btn !== this) btn.classList.remove("rotate");
        });

        // Toggle current sub-dropdown
        subSubmenu.style.maxHeight = isOpen ? "0px" : subSubmenu.scrollHeight + "px";
        this.classList.toggle("rotate", !isOpen);
    });
});
    

