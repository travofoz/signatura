            function filterOutNoisyClassNames(className) {
                function doesClassNameStartWithPrefix(badPrefix) {
                    return className.indexOf(badPrefix) === 0;
                }
            
                return !INTERNAL_CLASS_PREFIXES.some(doesClassNameStartWithPrefix);
